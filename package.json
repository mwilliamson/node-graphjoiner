{
  "name": "graphjoiner",
  "version": "0.4.0",
  "description": "Implementing GraphQL with joins",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha 'test/**/*.test.js'",
    "prepublish": "npm run build",
    "build": "babel src --out-dir lib --source-maps",
    "watch": "npm run build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mwilliamson/node-graphjoiner.git"
  },
  "keywords": [
    "graphql",
    "graph",
    "sql",
    "join"
  ],
  "author": "Michael Williamson <mike@zwobble.org>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/mwilliamson/node-graphjoiner/issues"
  },
  "homepage": "https://github.com/mwilliamson/node-graphjoiner",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "bluebird": "^3.4.6",
    "sql-gen": "^0.1.1",
    "mocha": "^3.0.2",
    "source-map-support": "^0.4.2",
    "sqlite3": "^3.1.4"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "ignore": [
      "lib/**/*"
    ]
  },
  "dependencies": {
    "graphql": "^0.10.1",
    "immutable": "^3.8.1",
    "lodash": "^4.15.0"
  }
}
